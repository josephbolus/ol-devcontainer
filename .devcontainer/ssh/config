Host mysql-primary
  HostName mysql-primary
  User dev
  IdentityFile ~/.ssh/devcontainer-mysql-ed25519
  IdentitiesOnly yes
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  LogLevel ERROR

Host mysql-replica
  HostName mysql-replica
  User dev
  IdentityFile ~/.ssh/devcontainer-mysql-ed25519
  IdentitiesOnly yes
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  LogLevel ERROR
